#container	
	%h1 Hi there!
	%p Welcome to Greenpop's Travel for Trees Carbon Calculator. Find your event or calculate without one.
	%ul 
		%li
			= search_field_tag("event_search", params[:event_search], :placeholder =>"Search...")
			= submit_tag("Go", class: "button")
		%li
			= link_to("Calculate without an event", "events/1")
	%ul
	- @events.each do |event|
		%li 
			= link_to(event.name, event)
			#{event.start_date}